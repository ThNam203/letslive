{
  "services": [
    {
      "name": "minio",
      "id": "letslive_minio",
      "address": "minio",
      "port": 9000,
      "check": {
        "id": "minio-check",
        "name": "MinIO Health Check",
        "tcp": "minio:9000",
        "interval": "30s",
        "timeout": "2s"
      }
    },
    {
      "name": "configserver",
      "id": "letslive_configserver",
      "address": "http://configserver",
      "port": 8181,
      "check": {
        "id": "configserver-check",
        "name": "Config Server Health Check",
        "tcp": "configserver:8181",
        "interval": "30s",
        "timeout": "2s"
      }
    }
  ],
  "recursors": ["127.0.0.11"]
}
